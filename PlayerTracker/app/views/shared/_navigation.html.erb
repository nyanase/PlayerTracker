<header x-data="{ open: false }" class="py-6 relative z-10 pwa:hidden">
  <div class="mx-auto px-4 sm:px-6">
<% link_cls = "flex items-center font-medium text-black hover:text-primary-500 focus:outline-none focus:text-gray-900 transition duration-150 ease-in-out"%>
    <nav class="relative flex items-center justify-between sm:h-10 md:justify-center z-10" aria-label="Main navigation">
      <div class="flex items-center flex-1 md:absolute md:inset-y-0 md:left-0">
        <div class="flex items-center justify-between w-full md:w-auto">
          <a href="/" class="text-black dark:text-white">
            <svg class="fill-current h-8 w-8 mr-2" width="54" height="54" viewBox="0 0 54 54" xmlns="http://www.w3.org/2000/svg"><path d="M13.5 22.1c1.8-7.2 6.3-10.8 13.5-10.8 10.8 0 12.15 8.1 17.55 9.45 3.6.9 6.75-.45 9.45-4.05-1.8 7.2-6.3 10.8-13.5 10.8-10.8 0-12.15-8.1-17.55-9.45-3.6-.9-6.75.45-9.45 4.05zM0 38.3c1.8-7.2 6.3-10.8 13.5-10.8 10.8 0 12.15 8.1 17.55 9.45 3.6.9 6.75-.45 9.45-4.05-1.8 7.2-6.3 10.8-13.5 10.8-10.8 0-12.15-8.1-17.55-9.45-3.6-.9-6.75.45-9.45 4.05z"/></svg>
          </a>
        </div>
      </div>

      <div class="hidden md:absolute md:flex md:items-center md:justify-end md:inset-y-0 md:right-0 space-x-8">
        <% if user_signed_in? %>
          <%= link_to('Logout', destroy_user_session_path, method: :delete, class: link_cls) %>
        <% else %>
          <%= link_to('Log In', new_user_session_path, class: link_cls) %>
          <%= link_to('Sign Up', new_user_registration_path, class: link_cls) %>
        <% end %>
        
      </div>
    </nav>
    </div>
  </header>
  

  